{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"app-plus": {
					"titleNView": false //去掉app+h5顶部导航
				}
			}
		}
		,{
		    "path" : "pages/clock/clock",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "时钟",
		        "enablePullDownRefresh": false,
				"app-plus":{
				     "titleNView":{
				      "buttons":[{
				       "color":"#ff6a6c",
				       "fontSize":"55rpx",
				       "text":"\ue641wish",
				       "fontSrc":"/static/uniicons.ttf",
					   "float":"left",
					   "name":"wish"
				      }]
				     }
				    }
		    }
		    
		},{
		    "path" : "pages/clock/wish/wish",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "心愿单",
		        "enablePullDownRefresh": false
		    }
		    
		},{
		    "path" : "pages/add/add",
		    "style": {
		    	"app-plus": {
		    		"titleNView": false //去掉app+h5顶部导航
		    	},
		    	"navigationBarTitleText": "",
		    	"enablePullDownRefresh": false
		    }
		    
		},{
		    "path" : "pages/add/addNote/addNote",
		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "写日记",
				"app-plus": {
					"titleNView": false //去掉app+h5顶部导航
				},
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
		    }
		    
		},{
            "path" : "pages/my/my",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "我的",
                "enablePullDownRefresh": false,
				"app-plus": {
					"titleNView": false //去掉app+h5顶部导航
				}
            }
            
        }
        
        ,{
            "path" : "pages/setting/setting",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "设置",
                "enablePullDownRefresh": false
            }  
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"uniIdRouter": {
		
	},
	"tabBar":{
	  	"color":"#333333",
	  	"selectedColor":"#FC5C82",
	  	"backgroundColor":"#FFFFFF",
	  	"borderStyle":"black",
	  	"list":[{
	  		"pagePath":"pages/clock/clock",
	  		"text":"时之钟",
	  		"iconPath":"static/tabbar/shijian.png",
	  		"selectedIconPath":"static/tabbar/shijianed.png"
	  	},{
	  		"pagePath":"pages/add/add",
	  		"text":"写什么",
	  		"iconPath":"static/tabbar/add.png",
	  		"selectedIconPath":"static/tabbar/added.png"
	  	},{
	  		"pagePath":"pages/my/my",
	  		"text":"这是我",
	  		"iconPath":"static/tabbar/my.png",
	  		"selectedIconPath":"static/tabbar/myed.png"
	  	}]
    }
